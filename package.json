{
  "name": "valcher",
  "version": "1.0.0",
  "description": "Valcher a launcher for valorant",
  "scripts": {
    "build:client": "cd ./src/client && npm run build",
    "build": "npm run build:client && tsc",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./dist/main.js",
    "dev:client": "cd ./src/client && npm run dev",
    "dev:server": "tsc-watch --outDir ./dist --onSuccess \"electron ./dist/main.js\"",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" "
  },
  "repository": "",
  "keywords": [],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^7.4.0",
    "electron": "^20.2.0",
    "eslint": "^7.32.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  }
}
