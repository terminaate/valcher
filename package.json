{
  "name": "valcher",
  "version": "1.0.0",
  "description": "Valcher a launcher for valorant",
  "main": "dist/main.js",
  "scripts": {
    "setup": "npm i && cd ./src/client && npm i",
    "build:client": "cd ./src/client && npm run build",
    "build": "npm run build:client && tsc",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "electron-forge start",
    "dev:client": "cd ./src/client && npm run dev",
    "dev:client:server": "cd ./src/client && npm run dev:server",
    "dev:server": "tsc-watch --outDir ./dist --onSuccess \"electron ./dist/main.js\"",
    "dev:http-server": "tsc-watch ./src/server/index.ts --outDir ./dist/server --onSuccess \"node ./dist/server/index.js\"",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" ",
    "package": "electron-forge package",
    "make": "npm run build && electron-forge make"
  },
  "repository": "",
  "keywords": [],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^7.4.0",
    "electron": "^20.2.0",
    "eslint": "^7.32.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@liamcottle/valorant.js": "^1.5.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "valorant.ts": "^1.4.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "valcher"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
